$group

    [ modules
        _ path.resolve(__dirname, "src")
        @ "node_modules"
	
    [ extensions
		@ ".js"
		$if pkg.Build.useTypescript
			@ ".ts"
		$if pkg.Build.useReact
			@ ".jsx"
			$if pkg.Build.useTypescript
				@ ".tsx"
		@ ".json"
		$if pkg.Build.useVue
			@ ".vue"
	
    { alias
		$if pkg.Build.useVue
			@ '@' resolve('src')
