$group
	
    "@babel/proposal-class-properties"
    "@babel/proposal-object-rest-spread"
    $if app.Build.usePrismjs
        [
            "prismjs"
            {
                languages
                    [
                        $foreach m in app.Build.prismjs.modes
                            "${m}"
                plugins ["line-numbers"]
                theme "twilight"
                css true
    $if app.Build.useDinamicImport
        "@babel/plugin-syntax-dynamic-import"